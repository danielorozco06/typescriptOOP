  },
  "homepage": "https://github.com/danielorozco06/oopTypescript#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/node-fetch": "^2.5.7",
    "@types/q": "^1.5.4",
    "@types/prettier": "2.4.4",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "jest-sonar": "^0.2.11",
    "ts-node": "^8.10.1",
    "open-cli": "^7.0.1",
    "typescript": "^4.7.4"
  }
